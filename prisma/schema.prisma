generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int       @id @default(autoincrement())
  id_persons Int?
  username   String?   @db.VarChar(255)
  email      String?   @unique
  password   String?   @db.VarChar(255)
  fullname   String?   @db.VarChar(255)
  phone      String?   @db.VarChar(255)
  created_at DateTime? @db.Timestamp(0)
  created_by String?   @db.VarChar(225)
  updated_at DateTime? @db.Timestamp(0)
  updated_by String?   @db.VarChar(225)
}

model persons {
  id         Int       @id @default(autoincrement())
  bdate      String?   @db.VarChar(225)
  gender     String?   @db.VarChar(225)
  photos     String?   @db.VarChar(225)
  education  String?   @db.VarChar(225)
  address    String?   @db.VarChar(225)
  notes      String?   @db.VarChar(225)
  is_deleted Int?
  created_at DateTime? @db.Timestamp(0)
  created_by String?   @db.VarChar(225)
  updated_at DateTime? @db.Timestamp(0)
  updated_by String?   @db.VarChar(225)
}
